.medaillon-container {
    width: 100%;
    height: 80%;
    flex-direction: row;
    justify-content: flex-start;
    align-items: flex-start;
    flex-wrap: wrap;
}

.title-menu {
    align-items: center;
    left: 25%;
    -unity-text-align: upper-center;
    width: 50%;
    right: auto;
    position: relative;
    white-space: normal;
    height: 10%;
    margin-left: 0;
    margin-right: 0;
    font-size: 16px;
    color: rgb(0, 0, 0);
    -unity-font-style: bold;
    -unity-font: resource('Fonts & Materials/LiberationSans');
    flex-wrap: wrap;
    max-height: 50px;
    min-height: 50px;
    -unity-font-definition: resource('Fonts & Materials/LiberationSans SDF');
}

.main-menu {
    height: 50%;
    width: 80%;
    background-color: rgba(130, 130, 130, 0.39);
    left: 10%;
    right: 10%;
    top: 25%;
    flex-shrink: 0;
    border-left-color: rgb(255, 8, 8);
    border-right-color: rgb(255, 8, 8);
    border-top-color: rgb(255, 8, 8);
    border-bottom-color: rgb(255, 8, 8);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    flex-wrap: wrap;
    align-items: flex-start;
    flex-grow: 0;
    max-height: none;
    min-height: 50%;
    max-width: 80%;
    min-width: 80%;
    display: flex;
}

.toto {
    color: rgb(0, 0, 0);
    -unity-font-style: bold;
    -unity-font: resource('');
    font-size: 19px;
    height: 7%;
    top: 2%;
    justify-content: center;
    align-items: center;
    flex-wrap: wrap;
    -unity-text-align: upper-center;
    white-space: normal;
    width: 50%;
    left: 25%;
    flex-shrink: 1;
    flex-direction: column;
    display: flex;
}

.ok-but {
    left: 50%;
    bottom: 1%;
    overflow: hidden;
    background-color: rgb(8, 241, 0);
    -unity-font-style: bold;
}
