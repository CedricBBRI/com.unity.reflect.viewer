    margin-right: 4px;
    margin-top: 4px;
    margin-bottom: 4px;
    max-width: none;
    max-height: none;
    min-height: auto;
    min-width: auto;
}

.selected-medaillon {
    height: 50px;
    width: 50px;
    border-top-left-radius: 10px;
